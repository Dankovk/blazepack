{
  "name": "blazepack",
  "version": "0.0.35",
  "description": "Blazing fast dev server powered by smooshpack",
  "main": "./bin/blazepack.js",
  "bin": {
    "blazepack": "./bin/blazepack.js"
  },
  "scripts": {
    "e2e": "jest e2e --forceExit",
    "e2e:ci": "jest e2e --forceExit --runInBand",
    "test": "jest src",
    "test:watch": "jest src --watch",
    "build-client": "parcel build src/client/index.js --out-dir src/lib",
    "dev-client": "parcel src/client/index.js --out-dir src/lib",
    "prepublish": "npm run build-client",
    "postinstall": "husky install",
    "prepare": "husky install",
    "prettify": "pretty-quick --staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ameerthehacker/blazepack.git"
  },
  "keywords": [
    "codesandbox",
    "dev",
    "server",
    "smooshpack"
  ],
  "author": "Ameer Jhan",
  "license": "GPL",
  "bugs": {
    "url": "https://github.com/ameerthehacker/blazepack/issues"
  },
  "homepage": "https://github.com/ameerthehacker/blazepack#readme",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "all-contributors-cli": "^6.20.0",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "jest-puppeteer": "^4.4.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "puppeteer": "^8.0.0",
    "rimraf": "^3.0.2",
    "wait-port": "^0.2.9"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "chokidar": "^3.5.1",
    "cli-select": "^1.1.2",
    "detect-indent": "^6.0.0",
    "find-package-json": "^1.2.0",
    "get-all-files": "^3.0.0",
    "latest-version": "^5.1.0",
    "minimist": "^1.2.5",
    "open": "^7.4.1",
    "update-notifier": "^5.1.0",
    "ws": "^7.4.3"
  }
}
